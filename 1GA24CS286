#include<stdio.h>
#include<stdlib.h>
#define MAX 5
void push(int s[],int *top,int item)
{
   if(*top==MAX-1)
   printf("Stack overflow\n");
   else{
   *top=*top+1;
   s[*top]=item;
   }
   }
   void pop(int s[],int *top)
   {
   if(*top==-1)
   printf("stack underflow\n");
   else{
   printf("popped element is %d\n",s[*top]);
   *top=*top-1;
   }
   }
   void display(int s[],int*top)
   {
   if(*top==1)
   printf("stack empty\n");
   else{
   for(int=*top;i>0:i--)
   printf("%d\n"s[i]);
   }
   }
   int main(){
   int s[MAX],top=-1,ch,item;
   for(;;)
   {
   printf("\n\push 2.pop. 3.display 4.exit\n);
   printf("Read choice");
   scanf("%d",&ch);
   switch(ch)
   {
   case 1:printf("\n Enter element to be copied:");
   scanf("%d",&item);
   push(&,&top,item);
   break;
   case 2:pop(s,&top);
   break;
   case 3:display(s,&top);
   break;
   default;
   exit(0);
   }
}
retrun0;
}
     
